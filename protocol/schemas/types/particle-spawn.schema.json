{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "particle-spawn.schema.json",
  "title": "ParticleSpawn",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "particle": {
      "type": "string",
      "minLength": 1
    },
    "x": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "y": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "z": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "count": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5000
    }
  },
  "required": ["particle"]
}
