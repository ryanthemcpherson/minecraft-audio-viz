# AudioViz Configuration
# Copy this file to .env and customize for your setup

# Minecraft Server Connection
MINECRAFT_HOST=localhost
MINECRAFT_PORT=8765

# VJ Server (for multi-DJ mode)
VJ_SERVER_PORT=9000

# Preview Server
PREVIEW_PORT=8766
HTTP_PORT=8080

# DJ Authentication (for VJ server mode)
DJ_AUTH_FILE=configs/dj_auth.json

# Audio Settings (optional)
# AUDIO_APP=spotify
# LOW_LATENCY=false

# Debug (optional)
# LOG_LEVEL=INFO
